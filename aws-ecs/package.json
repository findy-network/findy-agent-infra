    "jest": "^29.7.0",
    "js-green-licenses": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.129.0",
    "constructs": "^10.3.0"
  },
