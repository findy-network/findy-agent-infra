{
  "name": "findy-agency",
  "version": "0.1.2",
  "bin": {
    "findy-agency": "bin/findy-agency.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "licenses:check": "node ./tools/check-licenses.js ",
