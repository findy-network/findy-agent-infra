  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "18.6.5",
    "@types/prettier": "2.7.0",
    "aws-cdk": "2.29.1",
    "jest": "^27.5.1",
    "js-green-licenses": "^3.0.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.37.0",
    "constructs": "^10.1.71"
  },
